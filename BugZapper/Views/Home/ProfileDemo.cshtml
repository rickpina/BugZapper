@model BugZapper.Models.LoginModel

@{
    ViewData["Title"] = "ProfileDemo";
    Layout = "~/Views/Shared/_GuestLayout.cshtml";
}

<h1>Guest Profile</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Username)
        </dt>
        <dd class = "col-sm-10">
            <p>Guest</p>
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class = "col-sm-10">
            <p>null</p>
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-outline-info" asp-action="Index">Back to Home</a>
</div>
